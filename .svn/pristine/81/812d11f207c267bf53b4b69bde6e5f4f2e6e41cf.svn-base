}
.terms .header .header-content .left .en {
  padding-top: 20rpx;
  font-size: 24rpx;
  font-weight: normal;
  line-height: 45rpx;
  letter-spacing: 1.8rpx;
  color: #ffffff;
}
.terms .header .header-content .right {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
